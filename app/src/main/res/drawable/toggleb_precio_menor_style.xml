<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <rotate
            android:fromDegrees="180"
            android:toDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%"
            android:drawable="@drawable/ic_precio_mayor_off"/>
    </item>
    <item android:state_checked="true">
        <rotate
            android:fromDegrees="180"
            android:toDegrees="0"
            android:pivotX="50%"
            android:pivotY="50%"
            android:drawable="@drawable/ic_precio_mayor_on"/>
    </item>
</selector>